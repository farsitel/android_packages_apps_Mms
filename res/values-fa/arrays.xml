        <item>باحال</item>                    <!-- 7: B-) -->
        <item>پول</item>             <!-- 8: :-$ -->
        <item>عصبانی</item>           <!-- 9: :-! -->
        <item>خجالت‌زده</item>             <!-- 10: :-[ -->
        <item>فرشته</item>                   <!-- 11: O:-) -->
        <item>مردد</item>               <!-- 12: :-\\ -->
        <item>گریه</item>                  <!-- 13: :\'( -->
        <item>رازدار</item>         <!-- 14: :-X -->
